[maxscale]
threads=###threads###

[server1]
type=server
address=###node_server_IP_1###
port=###node_server_port_1###
protocol=MySQLBackend

[MySQL Monitor]
type=monitor
module=mysqlmon
###repl51###
servers=server1
user=maxskysql
passwd=skysql
monitor_interval=1000

[readconn]
type=service
router=readconnroute
servers=server1
user=maxskysql
passwd=skysql
filters=ignore

[readconn-listener]
type=listener
service=readconn
protocol=MySQLClient
port=4009

[ignore]
type=filter
module=dbfwfilter
rules=/###access_homedir###/rules/rules.txt
action=ignore
log_match=true
log_no_match=true
