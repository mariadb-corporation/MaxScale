[maxscale]
threads=###threads###
log_info=1

###server###

[Monitor]
type=monitor
module=mariadbmon
servers=###server_line###
user=maxskysql
password=skysql
monitor_interval=1000

[service1]
type=service
router=readconnroute
targets=server1,server2
user=maxskysql
password=skysql

[service2]
type=service
router=readconnroute
targets=server3,server4
user=maxskysql
password=skysql
