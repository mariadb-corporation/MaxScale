select sum(1) from test.t1;
select avg(1) from test.t1;
select sum(avg(1)) from test.t1;
select my_function(1) from test.t1;
select my_function("1") from test.t1;
select * from test.t1 where 1 = 1;
select * from test.t1 where 1 >= 1;
select * from test.t1 where 1 <= 1;
select * from test.t1 where 1 != 1;
select * from test.t1 where 1 <> 1;
select function(*) from test.t1;
select insert(*) from test.t1;
