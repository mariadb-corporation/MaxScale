        "lodash": "^4.17.21",
        "moment-duration-format": "^2.3.2",
        "moment-locales-webpack-plugin": "^1.2.0",
        "monaco-editor": "^0.33.0",
        "monaco-editor-webpack-plugin": "^7.0.1",
