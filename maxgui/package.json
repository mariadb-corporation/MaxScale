        "d3-dag": "~0.10.0",
        "date-fns": "~2.30.0",
        "dbgate-query-splitter": "~4.9.3",
        "deep-diff": "~1.0.2",
        "deepmerge": "~4.3.1",
        "git-describe": "~4.1.1",
        "html2canvas": "~1.4.1",
        "immutability-helper": "~3.1.1",
        "localforage": "~1.10.0",
