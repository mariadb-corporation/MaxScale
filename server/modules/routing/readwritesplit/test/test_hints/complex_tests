select @@server_id; -- maxscale master_route prepare route to master:
select @@server_id; -- maxscale begin master_route:3000
select @@server_id; -- maxscale max_slave_replication_lag = 100:3000
select @@server_id; -- maxscale begin route to server server3:3002
select @@server_id; -- maxscale max_slave_replication_lag = 100:3002
select @@server_id; -- maxscale end:3000
select @@server_id; -- maxscale max_slave_replication_lag = 100:3000
select @@server_id; -- maxscale end:3000
