[maxscale]
threads=###threads###
log_warning=1
#log_info=1

[server1]
type=server
address=###node_server_IP_1###
port=###node_server_port_1###
protocol=MySQLBackend

[MySQL-Monitor]
type=monitor
module=mysqlmon
servers=server1
user=maxskysql
password=skysql
monitor_interval=1000

[Cache]
type=filter
module=cache
enabled=true
invalidate=current
cached_data=shared
users=mixed
#debug=31

[RWS]
type=service
router= readwritesplit
servers=server1
user=maxskysql
password=skysql
filters=Cache

[RWS-Listener]
type=listener
service=RWS
protocol=MySQLClient
port=4006
