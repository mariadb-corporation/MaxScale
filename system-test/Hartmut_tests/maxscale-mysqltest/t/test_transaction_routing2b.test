--source testconf.inc
USE test;
--disable_warnings
DROP TABLE IF EXISTS myCity;
--enable_warnings
SET autocommit = Off;
START TRANSACTION;
CREATE TABLE myCity (a int, b char(20));
INSERT INTO myCity VALUES (1, 'Milan');
INSERT INTO myCity VALUES (2, 'London');
COMMIT;
START TRANSACTION;
DELETE FROM myCity;
SELECT COUNT(*) FROM myCity; # read transaction's modifications from master
COMMIT;
DROP TABLE myCity;
